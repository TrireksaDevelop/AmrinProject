<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             Title="Foto" BackgroundColor="{StaticResource colorPrimaryDark}"
             x:Class="MobileApp.Views.Contents.SertifikatView">
    <ContentPage.Content>
        <StackLayout Margin="5">
            <Image Margin="{Binding Photo}"></Image>
        </StackLayout>
    </ContentPage.Content>
</ContentPage>